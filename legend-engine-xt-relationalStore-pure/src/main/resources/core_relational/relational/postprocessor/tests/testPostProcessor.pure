function <<test.Test>> meta::relational::tests::postProcessor::testReAliasWindowColumn(): Boolean[1]
{
   let func = {|Order.all()->project([col(o|$o.id, 'id'), col(window([o|$o.zeroPnl,o|$o.id]), sortAsc(o|$o.quantity), y|$y->rank(), 'testCol')]) };
