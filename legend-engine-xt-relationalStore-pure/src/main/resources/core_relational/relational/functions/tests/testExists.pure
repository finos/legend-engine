function <<test.Test>> meta::relational::tests::query::filter::exists::testAssociationTwoLevelDeep():Boolean[1]
{
   let result = execute(|Person.all()->filter(p | $p.firm.employees->exists(e | $e.address.name == 'Hong Kong')), simpleRelationalMapping, testRuntime(), meta::pure::router::extension::defaultRelationalExtensions());
