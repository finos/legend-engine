      |Firm.all()->filter(f|$f.employees->exists(e|$e.firstName == 'Peter'))->project(col(x|$x.legalName, 'name')),
      simpleRelationalMapping, testRuntime(), meta::pure::router::extension::defaultRelationalExtensions()
   );
   assertEquals('name\nFirm X\n', $result1.values->meta::csv::toCSV());

   let result2 = execute(
      |Firm.all()->filter(f|$f.employees->exists(e|$e.lastName == 'Harris'))->project(col(x|$x.legalName, 'name')),
      simpleRelationalMapping, testRuntime(), meta::pure::router::extension::defaultRelationalExtensions()
   );
   assertEquals('name\nFirm C\n', $result2.values->meta::csv::toCSV());

   let result3 = execute(
      |Firm.all()->filter(f|$f.employees->exists(e|$e.firstName == 'Peter') || $f.employees->exists(e|$e.lastName == 'Harris'))->project(col(x|$x.legalName, 'name')),
      simpleRelationalMapping, testRuntime(), meta::pure::router::extension::defaultRelationalExtensions()
   );
   assertEquals('name\nFirm X\nFirm C\n', $result3.values->meta::csv::toCSV());
}