{
   $src->match(
                  [
                     s  : SelectSQLQuery[1] | $inFilter->if( | $s.filteringOperation->toOne(), | $s.columns->toOne()),
                     o  : SelectWithCursor[1] | $inFilter->if( | $o.select.filteringOperation->toOne(), | $o.select.columns->toOne()),
