Mapping meta::relational::tests::simpleRelationalMappingWithBooleanExpression
(
    Person : Relational
            {
                scope([dbInc])
                (
                    firstName : personTable.FIRSTNAME,
                    age : personTable.AGE
                ),
                scope([dbInc]default.personTable)
