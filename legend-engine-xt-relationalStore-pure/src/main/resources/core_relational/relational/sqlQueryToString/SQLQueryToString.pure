   let number = $p->at(1)->parseInteger();
   let unit = $p->at(2);

   'date(%s) %s %s %s'->format([
      $startDate,
      if ($number < 0 , | '-', | '+'),
      if($unit == 'WEEKS', | $number * 7, | $number)->abs()->toString(),
      if($unit == 'WEEKS', | 'DAYS', | $unit)
