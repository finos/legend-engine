   '       "enforcementLevel":"Error",'+
   '       "ruleDefinerPath":"meta::pure::mapping::modelToModel::test::alloy::dataQuality::dest::Review",'+
   '       "ruleType":"ClassConstraint",'+
   '       "path": ['+
   '         {'+
   '           "propertyName":"books",'+
   '           "index":0' +
   '         },'+
   '         {'+
   '           "propertyName":"marketing",'+
   '           "index":null' +
   '         },'+
