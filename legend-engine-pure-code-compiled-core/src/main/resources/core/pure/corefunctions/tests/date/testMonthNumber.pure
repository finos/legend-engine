function <<test.Test>> meta::pure::functions::date::tests::testMonthNumberForMonth():Boolean[1]
{
    assertEq(1, Month.January->monthNumber());
    assertEq(2, Month.February->monthNumber());
    assertEq(3, Month.March->monthNumber());
    assertEq(4, Month.April->monthNumber());
    assertEq(5, Month.May->monthNumber());
    assertEq(6, Month.June->monthNumber());
    assertEq(7, Month.July->monthNumber());
    assertEq(8, Month.August->monthNumber());
    assertEq(9, Month.September->monthNumber());
    assertEq(10, Month.October->monthNumber());
    assertEq(11, Month.November->monthNumber());
    assertEq(12, Month.December->monthNumber());
}

function <<test.Test>> meta::pure::functions::date::tests::testMonthForNumber():Boolean[1]
{
    assertEq(Month.January, 1->month());
    assertEq(Month.February, 2->month());
    assertEq(Month.March, 3->month());
    assertEq(Month.April, 4->month());
    assertEq(Month.May, 5->month());
    assertEq(Month.June, 6->month());
    assertEq(Month.July, 7->month());
    assertEq(Month.August, 8->month());
    assertEq(Month.September, 9->month());
    assertEq(Month.October, 10->month());
    assertEq(Month.November, 11->month());
    assertEq(Month.December, 12->month());
}
