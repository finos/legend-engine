function <<test.Test>> meta::pure::functions::date::tests::testDatePart():Boolean[1]
{
    assertEquals(%1973-11-05, %1973-11-05T13:01:25->datePart());
    assertEquals(%2015-08-29, %2015-08-29T22:22:22.9914234->datePart());
}

function <<test.Test>> meta::pure::functions::date::tests::testDatePartTrivial():Boolean[1]
{
    assertEquals(%1973-11-05, %1973-11-05->datePart());
    assertEquals(%2015-08-29, %2015-08-29->datePart());
}

function <<test.Test>> meta::pure::functions::date::tests::testDatePartYearMonthOnly():Boolean[1]
{
    assertEquals(%1973-11, %1973-11->datePart());
    assertEquals(%2015-08, %2015-08->datePart());
}

function <<test.Test>> meta::pure::functions::date::tests::testDatePartYearOnly():Boolean[1]
{
    assertEquals(%1973, %1973->datePart());
    assertEquals(%2015, %2015->datePart());
}
