import meta::pure::profiles::*;

function <<test.Test>> meta::pure::functions::multiplicity::tests::hasLowerBound::testHasLowerBound():Boolean[1]
{
    assertFalse(PureZero->hasLowerBound());
    assertFalse(ZeroOne->hasLowerBound());
    assert(PureOne->hasLowerBound());
    assertFalse(ZeroMany->hasLowerBound());
    assert(OneMany->hasLowerBound());
}

function <<test.Test>> meta::pure::functions::multiplicity::tests::hasLowerBound::testHasLowerBoundNonConcrete():Boolean[1]
{
    assertFalse(sort_T_m__T_m_->functionType().returnMultiplicity->hasLowerBound());
}