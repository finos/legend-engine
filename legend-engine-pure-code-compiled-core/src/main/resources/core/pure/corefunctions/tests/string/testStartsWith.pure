import meta::pure::profiles::*;

function <<test.Test>> meta::pure::functions::string::tests::startswith::testTrueStartsWith():Boolean[1]
{
    assert(startsWith('the quick brown fox jumps over the lazy dog', 'the'));
    assert(startsWith('the quick brown fox jumps over the lazy dog', 'the quick brown fox'));
}

function <<test.Test>> meta::pure::functions::string::tests::startswith::testFalseStartsWith():Boolean[1]
{
    assertFalse(startsWith('the quick brown fox jumps over the lazy dog', 'the lazy dog'));
}