   let originalMilestonedProperties = $class.originalMilestonedProperties->reverseMilestoningTransforms()->cast(@Property<Nil,Any|*>);

   ^meta::protocols::pure::v1_28_0::metamodel::domain::Class
   (
      _type = 'class',
      name = $class.name->toOne(),
      constraints = $class.constraints->map(c|$c->meta::protocols::pure::v1_28_0::transformation::fromPureGraph::domain::transformConstraint($useAppliedFunction, $extensions)),
      package = if($class.package->isEmpty(),|[],|$class.package->toOne()->elementToPath()),
