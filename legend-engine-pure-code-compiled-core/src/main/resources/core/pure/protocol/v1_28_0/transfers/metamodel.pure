function <<access.private>> meta::protocols::pure::v1_28_0::transformation::fromPureGraph::domain::enumPackage(e:Enumeration<Any>[1]):String[1]{
   let s = $e->elementToPath()->split('::');
   let fullLengh = $s->size();
   if($fullLengh < 2,
      |'',
      |$s->init()->joinStrings('::');
   );
}

