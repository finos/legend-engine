      post: PostfixExpression[1] | $post.op->in(['++', '--']),
      inf : InfixExpression[1]   | $inf.op->in(assignmentOperators()),
      mc  : MethodCall[1]        | true,
      smc : StaticMethodCall[1]  | true,
      new : New[1]               | true,
      c   : Code[1]              | false
   ])
}

