function meta::external::language::java::metamodel::isJavaLang(pkg: meta::external::language::java::metamodel::Package[1]):Boolean[1]
{
   $pkg.name == 'lang'
   && $pkg.parent->isNotEmpty()
   && $pkg.parent.name == 'java'
   && $pkg.parent.parent->isEmpty();
}

