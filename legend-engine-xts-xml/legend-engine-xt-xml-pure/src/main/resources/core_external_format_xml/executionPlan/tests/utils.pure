###Pure
import meta::legend::*;
import meta::pure::extension::*;
import meta::pure::runtime::*;
import meta::core::runtime::*;

function meta::external::format::xml::executionPlan::test::convertStringToBytes(data:String[1]): InstanceValue[1]
{
  ^InstanceValue(genericType=^GenericType(rawType=Byte), multiplicity=ZeroMany, values=$data)
}

function meta::external::format::xml::executionPlan::test::executeXsdSchemaBindingQuery(f: FunctionDefinition<Any>[1], vars: Pair<String, Any>[*]): String[1]
{
  executeLegendQuery($f, $vars, ^ExecutionContext(), meta::external::format::xml::executionPlan::test::xsdExtensions());
}

function meta::external::format::xml::executionPlan::test::xsdExtensions(): Extension[*]
{
  [
    meta::external::format::shared::externalFormatExtension(),
    meta::external::format::xml::extension::xsdFormatExtension()
  ]
}
