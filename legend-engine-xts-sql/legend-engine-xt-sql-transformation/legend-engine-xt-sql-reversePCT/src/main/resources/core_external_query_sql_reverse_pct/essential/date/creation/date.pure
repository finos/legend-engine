import meta::pure::test::pct::reversePCT::framework::*;
function meta::external::query::sql::reversePCT::tests::essential::date::creation::date::reverses():ReversesForSource[1]
{
  ^ReversesForSource(
    source = '/platform/pure/essential/date/creation/date.pure',
    reverses = [
                 revsForTest(
                      'meta::pure::functions::date::tests::testDateFromYear_Function_1__Boolean_1_',
                      [
                        revError('|1973->meta::pure::functions::date::date()',
                                 '',
                                 'Unexpected token',
