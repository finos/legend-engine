                      ]
                 ),
                 revsForTest(
                      'meta::pure::functions::date::tests::testDateFromSubSecond_Function_1__Boolean_1_',
                      [
                        rev('|1973->meta::pure::functions::date::date(11, 13, 23, 9, 11.0)',
                            'select make_timestamp(1973,11,13,23,9,11.0)'
                        ),
                        rev('|2015->meta::pure::functions::date::date(4, 16, 14, 51, 59.999)',
                            'select make_timestamp(2015,4,16,14,51,59.999)'
                        ),
                        rev('|2015->meta::pure::functions::date::date(4, 16, 14, 51, 59.999D)',
                            'select make_timestamp(2015,4,16,14,51,59.999)'
                        )
                      ]
                 )
