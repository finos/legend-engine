import meta::pure::test::pct::reversePCT::framework::*;

function meta::external::query::sql::reversePCT::tests::standard::math::aggregator::varianceSample::reverses():ReversesForSource[1]
{
  ^ReversesForSource(
    source = '/core_functions_standard/math/aggregator/varianceSample.pure',
    reverses = [
                 revsForTest(
                      'meta::pure::functions::math::tests::variance::testVarianceSample_Function_1__Boolean_1_',
                      [
                        rev('|[1.0, 2.0, 3.0]->meta::pure::functions::math::varianceSample()',
                            'select var_samp([1.0,2.0,3.0])'
                        ),
                        rev('|[2, 4, 6]->meta::pure::functions::math::varianceSample()',
                            'select var_samp([2,4,6])'
                        )
                      ]
                 ),
                 revsForTest(
                      'meta::pure::functions::math::tests::variance::testSimpleGroupByVarianceSample_Function_1__Boolean_1_',
                      [
                        rev('|#TDS{\n'+
                            'id,val\n'+
                            '1,1.0\n'+
                            '1,2.0\n'+
                            '1,3.0\n'+
                            '2,2\n'+
                            '2,4\n'+
                            '2,6}#->meta::pure::functions::relation::groupBy(~[id], ~[newCol:x: (id:Integer, val:Float)[1]|$x.val:y: Float[*]|$y->meta::pure::functions::math::varianceSample()])',
                            'select "table0_0".id as "id", var_samp("table0_0".val) as "newCol" from csv(\'id,val\n1,1.0\n1,2.0\n1,3.0\n2,2\n2,4\n2,6\') as "table0_0" group by "id"'
                        )
                      ]
                 ),
                 revsForTest(
                      'meta::pure::functions::math::tests::variance::testSimpleWindowVarianceSample_Function_1__Boolean_1_',
                      [
                        rev('|#TDS{\n'+
                            'id,val\n'+
                            '1,1.0\n'+
                            '1,2.0\n'+
                            '1,3.0\n'+
                            '2,2\n'+
                            '2,4\n'+
                            '2,6}#->meta::pure::functions::relation::extend(~id->meta::pure::functions::relation::over(), ~newCol:{p: meta::pure::metamodel::relation::Relation<(id:Integer, val:Float)>[1], w: meta::pure::functions::relation::_Window<(id:Integer, val:Float)>[1], r: (id:Integer, val:Float)[1]|$r.val}:y: Float[*]|$y->meta::pure::functions::math::varianceSample())',
                            'select "table0_0".id as "id", "table0_0".val as "val", var_samp("table0_0".val) OVER (Partition By "table0_0".id) as "newCol" from csv(\'id,val\n1,1.0\n1,2.0\n1,3.0\n2,2\n2,4\n2,6\') as "table0_0"'
                        )
                      ]
                 )
               ]
  )
}

