    source = '/core_functions_standard/collection/or.pure',
    reverses = [
                 revsForTest(
                      'meta::pure::functions::collection::tests::or::testOr_Function_1__Boolean_1_',
                      [
                        rev('|or(true)',
                            'select (Boolean\'true\')'
                        ),
                        rev('|or([true, true])',
                            'select (Boolean\'true\' or Boolean\'true\')'
                        ),
                        rev('|or([true, false])',
                            'select (Boolean\'true\' or Boolean\'false\')'
                        ),
                        rev('|or([true, true, false])',
                            'select (Boolean\'true\' or Boolean\'true\' or Boolean\'false\')'
                        ),
                        rev('|or(false)',
                            'select (Boolean\'false\')'
                        ),
                        rev('|or([false, false])',
                            'select (Boolean\'false\' or Boolean\'false\')'
                        )
                      ]
                 )
               ]
  )
}

