                    </dependency>
                    <dependency>
                        <groupId>org.finos.legend.engine</groupId>
                        <artifactId>legend-engine-pure-runtime-java-extension-compiled-functions-json</artifactId>
                        <version>${project.version}</version>
                    </dependency>

                </dependencies>
            </plugin>
        </plugins>
