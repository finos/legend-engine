function <<test.Test>> meta::external::query::sql::transformation::queryToPure::tests::testExpression():Boolean[1]
{
  let res = meta::legend::compile(readFile('/core_external_query_sql/binding/fromPure/tests/expression/query.legend')->toOne());
  meta::pure::executionPlan::executionPlan(
    $res->filter(p|$p->instanceOf(FunctionDefinition))->toOne()->cast(@FunctionDefinition<Any>),
    meta::pure::extension::runtime::getExtensions(),
    debug()
  );
  true;
}