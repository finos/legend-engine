      ->addTest('String : [0..1],[1]    : A > B',          {|['A']->first() > 'B'}, 'org.finos.legend.engine.plan.dependencies.util.Library.safeCompare(org.finos.legend.engine.plan.dependencies.util.Library.first("A"), "B") > 0', javaBoolean())->assert('(%s)==false')
