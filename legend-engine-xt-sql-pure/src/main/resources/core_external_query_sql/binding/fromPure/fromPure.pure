      pair($simplified.right->instanceOf(IntervalLiteral) && !$simplified.left->instanceOf(NullLiteral), {|
        let left = $simplified.left->processExpression($expContext, $context);
        processIntervalToAdjust($left, $simplified.right->cast(@IntervalLiteral), $negate);}),
      pair($simplified.right->instanceOf(NullLiteral) || $simplified.left->instanceOf(NullLiteral), {|
