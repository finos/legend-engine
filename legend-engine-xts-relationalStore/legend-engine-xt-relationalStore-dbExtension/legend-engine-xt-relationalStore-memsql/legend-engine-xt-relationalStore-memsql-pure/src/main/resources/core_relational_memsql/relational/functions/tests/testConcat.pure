import meta::relational::tests::model::simple::*;
import meta::relational::functions::sqlstring::*;

function <<test.Test>> meta::relational::memsql::tests::query::concat::testConcat_MemSQL():Boolean[1]
{
  let s = toSQLString(|Person.all()->project([col(p|$p.firstName, 'firstName'), col(p|$p.lastName, 'lastName')])->meta::pure::tds::extensions::extendWithDigestOnColumns('hash', meta::pure::functions::hash::HashType.MD5, ['firstName', 'lastName']);, meta::relational::tests::simpleRelationalMapping, meta::relational::runtime::DatabaseType.MemSQL, meta::relational::extension::relationalExtensions());
  assertEquals('select `root`.FIRSTNAME as `firstName`, `root`.LASTNAME as `lastName`, md5(concat_ws(\'|\', cast(`root`.FIRSTNAME as char), cast(`root`.LASTNAME as char))) as `hash` from personTable as `root`', $s);
}
