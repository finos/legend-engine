                        <version>${legend.pure.version}</version>
                    </dependency>
                    <dependency>
                        <groupId>org.finos.legend.pure</groupId>
                        <artifactId>legend-pure-m2-store-relational-grammar</artifactId>
                        <version>${legend.pure.version}</version>
                    </dependency>
                </dependencies>
            </plugin>
        </plugins>
    </build>

