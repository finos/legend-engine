    {
       legalName: [FCTtestDB]TEST.FIRM.LEGAL_NAME,
       optionalDescription: [FCTtestDB]TEST.FIRM.OPTIONAL_DESCRIPTION,
       employees: [FCTtestDB]@personFirm
    }
)


