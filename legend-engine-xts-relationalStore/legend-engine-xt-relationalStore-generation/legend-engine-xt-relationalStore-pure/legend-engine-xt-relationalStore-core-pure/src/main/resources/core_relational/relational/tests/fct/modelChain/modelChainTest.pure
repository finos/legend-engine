import meta::relational::tests::mapping::modelChain::model::mapping::fct::store::*;
Mapping meta::relational::tests::mapping::modelChain::model::mapping::fct::modelChainTestMapping
(
    S_Person: Relational
    {
       scope([FCTtestDB]TEST.PERSON)
       (
          firstName: FIRSTNAME,
          lastName : LASTNAME,
          birthDate: BIRTHDATE,
          address(
            street:STREET
          )      
       ),
       firm: [FCTtestDB]@personFirm
    }


    S_Firm: Relational
    {
       legalName: [FCTtestDB]TEST.FIRM.LEGAL_NAME,
       optionalDescription: [FCTtestDB]TEST.FIRM.OPTIONAL_DESCRIPTION,
       employees: [FCTtestDB]@personFirm
    }
)


###Relational

Database meta::relational::tests::mapping::modelChain::model::mapping::fct::store::FCTtestDB
(
    Schema TEST
    (
    Table PERSON
    (
        ID INT PRIMARY KEY,
        FIRSTNAME VARCHAR(20),
        LASTNAME VARCHAR(20),
        FIRM_ID INT,
        BIRTHDATE DATE,
        STREET VARCHAR(20)
    )

    Table FIRM
    (
       ID INT PRIMARY KEY,
       LEGAL_NAME VARCHAR(20),
       OPTIONAL_DESCRIPTION VARCHAR(20)

    )

    )
    Join personFirm(TEST.PERSON.FIRM_ID = TEST.FIRM.ID)

)
