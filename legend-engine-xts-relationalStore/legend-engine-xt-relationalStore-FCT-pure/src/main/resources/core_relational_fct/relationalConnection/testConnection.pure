function  meta::relational::fct::testGeneration::buildExpectedInstance(result:String[1],expected:String[1]):meta::protocols::pure::vX_X_X::metamodel::valueSpecification::raw::KeyExpression[1]
{ 
 let instance = if($expected == 'tds',
          |  let r =$result->meta::json::fromJSON(RelationalTDSResult, ^meta::json::ExtendedJSONDeserializationConfig(typeKeyName='__TYPE', failOnUnknownProperties=true, nullReplacementInArray=TDSNull))->cast(@RelationalTDSResult)->toOne()->meta::relational::fct::resultToCSV();
              ^TDSResult(value=$r)->deactivate() ->transformValueSpecification([],  [pair('r',list($r))] ->newMap(), true, false, relationalExtensions());,
          |  ^JSONResult(value=$result)->deactivate()->transformValueSpecification([],  [pair('result',list($result))] ->newMap(), true, false, relationalExtensions());
  );

    ^meta::protocols::pure::vX_X_X::metamodel::valueSpecification::raw::KeyExpression(_type='keyExpression',key=^CString(_type='string',value='expectedExecutionResult'),expression=$instance);

}




function   meta::relational::fct::testGeneration::testReference(function:ConcreteFunctionDefinition<Any>[1]):meta::protocols::pure::vX_X_X::metamodel::valueSpecification::raw::KeyExpression[1]
{ 

    ^meta::protocols::pure::vX_X_X::metamodel::valueSpecification::raw::KeyExpression(_type='keyExpression',key=^CString(_type='string',value='test'),expression=^meta::protocols::pure::vX_X_X::metamodel::valueSpecification::raw::PackageableElementPtr(_type ='packageableElementPtr',fullPath=$function->elementToPath()));

}



            