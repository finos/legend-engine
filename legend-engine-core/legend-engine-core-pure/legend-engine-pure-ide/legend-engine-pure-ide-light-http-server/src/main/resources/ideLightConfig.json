{
  "logging": {
    "level": "info",
    "appenders": [
      {
        "type": "console",
        "logFormat": "%msg\r\n"
      }
    ]
  },
  "swagger": {
    "title": "Legend Pure IDE Light",
    "resourcePackage": "org.finos.legend",
    "uriPrefix": "/"
  },
  "sessionCookie": "LEGEND_ENGINE_JSESSIONID",
  "server": {
    "type": "simple",
    "applicationContextPath": "/",
    "adminContextPath": "/admin",
    "connector": {
      "maxRequestHeaderSize": "32KiB",
      "type": "http",
      "port": 8090
    },
    "requestLog": {
      "appenders": [
      ]
    }
  },
  "debugMode" : true,
  "initPureRuntimeBeforeStart" : true,
  "sourceLocationConfiguration": {
    "welcomeFileDirectory": "./",
    "directories" : ["."],
    "pathPatternsToExclude" : ["**/target", "**/archetype-resources"]
  },
  "requiredRepositories" : []
}
