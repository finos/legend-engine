function <<PCT.test>> meta::pure::functions::relation::tests::extend::testOLAPAggNoWindow<T|m>(f:Function<{Function<{->T[m]}>[1]->T[m]}>[1]):Boolean[1]
{
    let expr = {
                | #TDS
                  id, grp, name
                  1, 2, A
                  2, 1, B
                  3, 3, C
                  4, 4, D
                  5, 2, E
                  6, 1, F
                  7, 3, G
                  8, 1, H
                  9, 5, I
                  10, 0, J
                #->extend(~newCol:c|$c.id:y|$y->plus())
               };

    let res =  $f->eval($expr);

    assertEquals( '#TDS\n'+
                  '   id,grp,name,newCol\n'+
                  '   1,2,A,55\n'+
                  '   2,1,B,55\n'+
                  '   3,3,C,55\n'+
                  '   4,4,D,55\n'+
                  '   5,2,E,55\n'+
                  '   6,1,F,55\n'+
                  '   7,3,G,55\n'+
                  '   8,1,H,55\n'+
                  '   9,5,I,55\n'+
                  '   10,0,J,55\n'+
                  '#', $res->sort(~id->ascending())->toString());
}

