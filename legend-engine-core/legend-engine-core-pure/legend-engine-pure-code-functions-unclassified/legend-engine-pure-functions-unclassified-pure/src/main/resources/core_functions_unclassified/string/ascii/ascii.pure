native function meta::pure::functions::string::ascii(source:String[1]):Integer[1];

function <<test.Test>> {test.excludePlatform = 'Java compiled'} meta::pure::functions::string::tests::ascii::testSimpleAscii():Boolean[1]
{
    assertEquals(0, ascii(''));
    assertEquals(32, ascii(' '));
    assertEquals(97, ascii('abc'));
}