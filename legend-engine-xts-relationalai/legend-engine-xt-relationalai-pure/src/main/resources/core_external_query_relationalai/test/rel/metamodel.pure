import meta::rel::metamodel::*;

function <<test.Test>> meta::rel::test::metamodel::testIdentifier(): Any[*] {
  assertEquals(
    'output', 
    rel('output')->identifier()
  );
  
  assertEquals(
    'output', 
    rel('output')
      ->appl([var('x'), var('y')])
      ->identifier()
  );

  assertEquals(
    'output', 
    rel('output')
      ->appl([var('x'), var('y')])
      ->appl([label('test')])
      ->identifier()
  );
}