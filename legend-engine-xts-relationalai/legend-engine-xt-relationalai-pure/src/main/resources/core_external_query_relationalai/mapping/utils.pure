import meta::pure::mapping::*;
import meta::external::store::rel::mapping::*;

function meta::external::store::rel::mapping::rel(activities: Activity[*]): String[1] {
  let relSource = $activities
    ->filter(a | $a->instanceOf(QueryActivity))
    ->toOne()
    ->cast(@QueryActivity)
    .relSource;

  $relSource;
}